{
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "rules": {
    "semicolon": [true, "never"],
    "ter-arrow-parens": false,
    "import-name": false,
    "max-line-length": false,
    "object-literal-shorthand": false
  },
  "linterOptions": {
    "exclude": ["src/migration/*", "test/__mocks__/*"]
  }
}